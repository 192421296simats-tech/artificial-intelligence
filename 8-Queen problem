N = 8

def print_board(board):
    for i in range(N):
        row = ""
        for j in range(N):
            row += "Q " if board[i] == j else ". "
        print(row)
    print()

def is_safe(board, row, col):
    for i in range(row):
        if board[i] == col or abs(board[i] - col) == abs(i - row):
            return False
    return True

def solve(row, board):
    if row == N:
        print_board(board)
        return True
    for col in range(N):
        if is_safe(board, row, col):
            board[row] = col
            if solve(row + 1, board):
                return True
    return False

board = [-1] * N
solve(0, board)
